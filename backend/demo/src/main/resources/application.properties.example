# AIML FastAPI endpoint for semantic relationships
aiml.base-url=http://localhost:8001

# pgvector storage for chunk embeddings
vector.store.table=text_chunk_embedding
vector.store.dimensions=384

# cosine similarity threshold for creating chunk-to-chunk semantic edges
semantic.threshold=0.8
